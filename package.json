    {
      "path": "dist/ui-ecosystem-life.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
